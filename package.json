{
  "name": "@frequency-chain/style-guide",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "src/index.js",
  "scripts": {
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build",
    "package": "svelte-package && cp 'src/styles/index.css' dist/ && cp 'tailwind.config.cjs' dist/ && cp 'postcss.config.cjs' dist/ && rimraf dist/**/*.stories.* && rimraf dist/**/*.mdx && find dist/* -type d -empty -delete",
    "dry-run": "npm pack --dry-run",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "npm run prettier && npm run lint:fix",
    "prettier": "prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss src",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.0",
    "@storybook/addon-essentials": "^7.0.0-beta.51",
    "@storybook/addon-interactions": "^7.0.0-beta.51",
    "@storybook/addon-links": "^7.0.0-beta.51",
    "@storybook/addon-svelte-csf": "^3.0.0-next.3",
    "@storybook/blocks": "^7.0.0-beta.51",
    "@storybook/svelte": "^7.0.0-beta.51",
    "@storybook/svelte-vite": "^7.0.0-beta.51",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-svelte": "^2.44.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^7.0.0-beta.51",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "svelte": "^3.55.1",
    "svelte-loader": "^3.1.6",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.0"
  },
  "dependencies": {
    "rollup-plugin-delete": "^2.1.0",
    "svelte": "^3.55.1"
  },
  "files": [
    "src",
    "dist",
    "tailwind.config.cjs",
    "postcss.config.cjs"
  ]
}
